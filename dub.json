{
	"name": "acos5_64_gui",
	"description": "Administration tool for ACOS5-64, based on driver acos5_64",
	"license": "GNU General Public License v2.0",
	"copyright": "Copyright © 2018-, Carsten Blüggel",
	"authors": ["Carsten Blüggel"],
	"homepage": "https://github.com/carblue/acos5_64_gui",
	"dependencies": {
		"iup": "~>3.27.0-alpha.1",
		"tasn1:wrapper": "~>0.0.5",
		"opensc": "~>0.19.0-alpha.1",
		"pkcs11": "~>2.40.0-alpha.3",
		"openssl": "~>2.0.0+1.1.0h"
	},
	"subConfigurations": {
		"opensc": "deimos",
		"pkcs11": "derelict"
	},
	"systemDependencies": "IUP-Portable User Interface shared libraries installed, OpenSC installation, acos5_64 driver and pkcs15init libraries installed, opensc.conf configured appropriately; (not yet required: for gettext the exports available from libintl.so or GLib)",
	"libs-posix": ["iupimglib"],
	"sourceFiles-windows-x86_64": [
		"C:/D/dmd2/windows/lib/ole32.lib",
		"oleaut32.lib",
		"uuid.lib"
	],
	"targetName": "acos5_64_gui",
	"mainSourceFile": "source/acos5_64_gui.d",

	"dflags-posix-dmd": ["-defaultlib=libphobos2.so"],
	"dflags-posix-ldc": ["-defaultlib=phobos2-ldc-shared,druntime-ldc-shared"],
	"fixme-dflagsx-posix-gdc": ["failes", "on", "slides"],

	"lflags": ["--no-undefined", "--as-needed", "--version-script=acos5_64_gui.ver"],
	"postBuildCommands": ["strip --strip-unneeded $$DUB_TARGET_NAME"],
	"versions": ["Have_acos5_64", "I18N"],
	"configurations": [
		{
			"name": "application",
	        "targetType": "executable",
            "excludedSourceFiles": [],
			"dflags": []
		},
		{
			"name": "travis",
	        "targetType": "executable",
            "excludedSourceFiles": [],
			"dflags": [],
			"buildRequirements": ["disallowDeprecations"]
		},
		{
			"name": "unittest",
			"targetType": "library",
			"dflags": []
		}
	]
}
